{
    "name": "tomatophp/filament-users",
    "type": "library",
    "description": "User Table Resource with a lot of packages integrations",
    "keywords": [
        "php",
        "laravel",
        "filamentphp",
        "users",
        "user CRUD",
        "user resource",
        "user management",
        "filament-shield",
        "filament-impersonate"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentUsers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomatoPHP\\FilamentUsers\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentUsers\\FilamentUsersServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "filament/filament": "^3.2"
    },
    "require-dev": {
        "livewire/livewire": "^2.10|^3.0",
        "orchestra/testbench": "^9.5",
        "pestphp/pest-plugin-livewire": "^1.0|^2.1",
        "spatie/pest-plugin-snapshots": "^1.1|^2.1"
    }
}
